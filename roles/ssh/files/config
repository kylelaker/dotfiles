VerifyHostKeyDNS yes
PubkeyAcceptedAlgorithms +ssh-rsa
HostkeyAlgorithms +ssh-rsa

Host localhost
    IdentityFile ~/.ssh/kyle_ed25519
Host github.com
    Hostname github.com
    User git
    IdentityFile ~/.ssh/github_ed25519
Host 192.168.*
    Ciphers +aes256-cbc
    #KexAlgorithms aes256-cbc
    IdentityFile ~/.ssh/homenet_ed25519
Host aur.archlinux.org
    IdentityFile ~/.ssh/aur_ed25519
    User aur
